<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="350.0" prefWidth="305.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="se.lu.ics.controllers.VehicleAddDialogController">

 <children>
        <Label fx:id="labelVehicleName" layoutX="20" layoutY="20" text="Name" />
        <TextField fx:id="textFieldVehicleName" layoutX="94.0" layoutY="16.0" prefWidth="150.0" />
    
        <Label fx:id="labelVehicleType" layoutX="20.0" layoutY="75" text="Type" />
        <ComboBox fx:id="comboBoxVehicleType" layoutX="94" layoutY="71" prefWidth="150.0" />

        <Label fx:id="labelVehicleLocation" layoutX="20.0" layoutY="130" text="Location" />
        <TextField fx:id="textFieldVehicleLocation" layoutX="94.0" layoutY="126.0" prefWidth="150.0" />

        <Label fx:id="labelVehicleCapacity" layoutX="20.0" layoutY="185" text="Capacity" />
        <TextField fx:id="textFieldVehicleCapacity" layoutX="94.0" layoutY="181.0" prefWidth="150.0" />
    
        <Label fx:id="labelVehicleWorkshop" layoutX="20.0" layoutY="240.0" text="Workshop" />
        <ComboBox fx:id="comboBoxVehicleWorkshop" layoutX="94.0" layoutY="236.0" prefWidth="150.0" /> 

        <Button fx:id="buttonVehicleSave" layoutX="130.0" layoutY="295.0" onAction="#handleButtonVehicleSaveAction" text="Save" />

        <Label fx:id="labelResponse" layoutX="20" layoutY="260" prefHeight="25.0" prefWidth="250.0" text="Response" visible="false" />
 </children>
</AnchorPane>
