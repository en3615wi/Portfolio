<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="295.0" prefWidth="305.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="se.lu.ics.controllers.MaintenanceScheduleAddDialogController">

 <children>
        <Label fx:id="labelVehicle" layoutX="20" layoutY="20" text="Vehicle" />
        <ComboBox fx:id="comboBoxVehicle" layoutX="94.0" layoutY="16.0" prefWidth="150.0" />

        <Label fx:id="labelWorkshop" layoutX="20.0" layoutY="75.0" text="Workshop" />
        <ComboBox fx:id="comboBoxWorkshop" layoutX="94.0" layoutY="71.0" prefWidth="150.0" />
        
        <Label fx:id="labelDate" layoutX="20.0" layoutY="130.0" text="Date" />
        <DatePicker fx:id="datePickerMaintenanceScheduleDate" layoutX="94.0" layoutY="126.0" prefWidth="150.0" />

        <Label fx:id="labelTime" layoutX="20.0" layoutY="185.0" text="Time" />
        <TextField fx:id="textFieldMaintenanceScheduleTime" layoutX="94.0" layoutY="181.0" prefWidth="150.0" />

        <Button fx:id="buttonMaintenanceScheduleSave" layoutX="130.0" layoutY="240.0" onAction="#handleButtonMaintenanceScheduleSaveAction" text="Save" />
        
        <Label fx:id="labelResponse" layoutX="20" layoutY="210" prefHeight="25.0" prefWidth="250.0" text="Response" visible="false" />
 </children>
</AnchorPane>
